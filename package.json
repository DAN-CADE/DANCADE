{
  "name": "arcade-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "server": "node server.js",
    "build:handlers": "tsc -p tsconfig.server.json",
    "watch:handlers": "tsc -p tsconfig.server.json --watch",
    "dev:all": "npm run build:handlers && concurrently \"npm run server\" \"npm run dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.36.5",
    "@hookform/resolvers": "^3.10.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.86.2",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "gsap": "^3.14.2",
    "next": "16.0.7",
    "phaser": "^3.90.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.70.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.3",
    "unique-names-generator": "^4.7.1",
